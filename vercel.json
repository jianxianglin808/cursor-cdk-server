{
  "version": 2,
  "routes": [
    {
      "src": "/api/get_auth",
      "dest": "/api/get_auth",
      "methods": ["POST"],
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "POST",
        "Access-Control-Allow-Headers": "Content-Type",
        "X-Force-Protocol": "http"
      }
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1",
      "methods": ["POST"],
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "POST",
        "Access-Control-Allow-Headers": "Content-Type"
      }
    }
  ],
  "functions": {
    "pages/api/*.js": {
      "maxDuration": 60
    }
  }
}
